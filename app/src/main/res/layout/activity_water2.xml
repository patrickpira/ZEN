<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Air2"
    android:background="@drawable/blurredbg">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="293dp"
        android:layout_height="166dp"
        android:fontFamily="sans-serif-black"
        android:text="\t\tNever clean up a spill by hosing it into a storm drain. Place kitty litter, sand, or another absorbent on the spill. Once the liquid becomes solid - sweep it up and throw it in a garbage can. "
        android:textColor="@color/white"
        android:textStyle="italic|bold"
        android:textSize="17dp"
        android:justificationMode="inter_word"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.187" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="292dp"
        android:layout_height="156dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.111"
        app:srcCompat="@drawable/trashtag_challenge" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="265dp"
        android:layout_height="175dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.89"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.467"
        app:srcCompat="@drawable/coastal_cleanup_month_1_889x592" />
</androidx.constraintlayout.widget.ConstraintLayout>